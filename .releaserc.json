{
  "branches": [
    "main",
    {
      "name": "test",
      "prerelease": true
    },
    {
      "name": "beta",
      "prerelease": "beta"
    },
    {
      "name": "alpha",
      "prerelease": "alpha"
    },
    {
      "name": "next",
      "prerelease": "rc"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    ["@semantic-release/exec", {
      "prepareCmd": "dotnet build -c Release && npm run package"
    }],
    ["@semantic-release/github", {
      "assets": [
        "release/*.zip",
        "README.md"
      ]
    }]
  ]
}